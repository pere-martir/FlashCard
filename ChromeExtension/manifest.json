{
  "name": "Tarjetas Flash",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Not available",
  "background": { "page": "background.html" }, 
  "permissions" : ["tabs", "http://*/*", "background"],
  "options_page" : "login.html",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-1.8.1.min.js", "tarjetasflash.js", "jquery.blockUI.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "Number-1-icon.png",
    "Number-2-icon.png",
    "Number-3-icon.png",
    "Number-4-icon.png",
    "Number-5-icon.png",
    "Number-6-icon.png",
    "Number-7-icon.png",
    "Number-8-icon.png",
    "Number-9-icon.png",
    "Math-plus-icon.png"
  ]
}
